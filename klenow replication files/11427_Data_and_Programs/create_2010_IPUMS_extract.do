  byte    sex       48-48  ///
  int     age       49-51  ///
  byte    race      52-52  ///
  int     raced     53-55  ///
  byte    educ      56-57  ///
  int     educd     58-60  ///
  byte    empstat   61-61  ///
  byte    empstatd  62-63  ///
  byte    labforce  64-64  ///
  int     occ       65-68  ///
  int     occ1990   69-71  ///
  byte    wkswork2  72-72  ///
  byte    uhrswork  73-74  ///
  long    incwage   75-80  ///
  long    incbus00  81-86  ///
  using   ipums_2012_main.dat

replace hhwt     = hhwt     / 100
replace perwt    = perwt    / 100

format serial   %8.0f
format hhwt     %10.2f
format perwt    %10.2f

