/*********************** Step 1:  Read in IPUMS Data **************************************/
/******************************************************************************************/
/******************************************************************************************/

set more off

cd c:\ErikMain\discrimination_growth  /* Need to update directly when running on a different computer */
set mem 5000m 


clear
quietly infix              ///
  int     year      1-4    ///
  byte    datanum   5-6    ///
  double  serial    7-14   ///
  float   hhwt      15-24  ///
  byte    statefip  25-26  ///
  byte    gq        27-27  ///
  int     pernum    28-31  ///
  float   perwt     32-41  ///
  byte    relate    42-43  ///
